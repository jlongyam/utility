{
  "source": {
    "include": [
      "./src",
      "./README.md",
      "./CHANGELOG.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "destination": "./docs",
    "readme": "./README.md",
    "recurse": true,
    "template": "./node_modules/jsdoc-baseline",
    "footer": ""
  }
}