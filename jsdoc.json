{
  "source": {
    "include": [
      "./src",
      "./README.md",
      "./CHANGELOG.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "./plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "./docs",
    "recurse": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "dark",
      "homepageTitle": "Utility - home",
      "title": "HOME",
      "menu": [
        {
          "title": "Github",
          "id": "github",
          "link": "https://github.com/jlongyam/utility/"
        },
        {
          "title": "npm",
          "id": "npm",
          "link": "https://www.npmjs.com/package/@jlongyam/utility"
        }
      ],
      "footer": "<p style='text-align: center'>Theme by <strong><a href='https://ankdev.me/clean-jsdoc-theme/v4/'>Clean-JSDoc</a></strong></p>"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}